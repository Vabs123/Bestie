{
 "manifest_version": 2,
"name": "Custom Google Homepage",
 "description": "This extension shows a Google Image search result for the current page",
 "version": "1.0",
   "background": {
        "scripts": [
            "background.js"
        ]
    },
"page_action": {
 "default_icon": "img.jpg",
 "default_popup": "popup.html",
 "default_title": "My custom google page!"
 },
 "permissions": [
    "webNavigation",
    "tabs",
    "storage",
    "idle"
]
}